system
  "olme"

root
  MAIN: make

default
  assertion (all)
  -- assertion (no) -- run fast, at the cost of skipping runtime contract checks
  collect (no)

cluster
  "./src"
  "${path_liberty_core}/loadpath.se"
  "${path_liberty_extra}/common/loadpath.se"
  "${path_liberty_extra}/posix/loadpath.se"
  "${path_liberty_extra}/readline/loadpath.se"

generate
  clean(yes);
